{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "roots": ["<rootDir>/src"],
  "testMatch": ["**/*.test.ts"],
  "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.test.ts"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "moduleNameMapper": {
    "^lodash-es$": "lodash"
  },
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.json"
      }
    ]
  }
}
